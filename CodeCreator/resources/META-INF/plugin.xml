<idea-plugin version="2">
    <id>com.pqixing.modularization.codeCreator</id>
    <name>CodeCreator</name>
    <version>1.1</version>
    <vendor email="support@yourcompany.com" url="http://www.yourcompany.com">YourCompany</vendor>

    <description><![CDATA[
      Enter short description for your plugin here.<br>
      <em>most HTML tags may be used</em>
    ]]></description>

    <change-notes><![CDATA[
      Fix Task ToMaven For Tag : Not Update <br>
      <em>most HTML tags may be used</em>
    ]]>
    </change-notes>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
    <idea-version since-build="141.0"/>

    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <!-- uncomment to enable plugin in all products
    <depends>com.intellij.modules.lang</depends>
    -->

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>


    <actions>
        <group class="com.dachen.creator.Modularization" popup="true" id="modularization" text="Modularization">
            <add-to-group group-id="BuildMenu" anchor="first"/>
            <add-to-group group-id="EditorPopupMenu" anchor="first"/>
            <add-to-group group-id="ProjectViewPopupMenu" anchor="first"/>
            <group class="com.dachen.creator.Git" popup="true" id="git" text="Git">

                <action id="CheckOut" class="com.dachen.creator.actions.CheckOut" text="CheckOut" description="CheckOut"/>

                <action id="Update" class="com.dachen.creator.actions.Update" text="Update" description="Update"/>
                <action id="Clone" class="com.dachen.creator.actions.Clone" text="Clone" description="Clone"/>
                <separator />
                <action id="CreateBranch" class="com.dachen.creator.actions.CreateBranch" text="CreateBranch"
                        description="CreateBranch"/>
            </group>

            <group class="com.dachen.creator.OpenFile" popup="true" id="file" text="OpenFile">
                <action id="OpenInclude" class="com.dachen.creator.actions.Include" text="OpenInclude"
                        description="OpenInclude">
                    <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt I"/>
                </action>
                <action id="OpenProperties" class="com.dachen.creator.actions.Properties" text="OpenProperties"
                        description="OpenProperties">
                    <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt shift P"/>
                </action>

                <action id="Dependencies" class="com.dachen.creator.actions.Dependencies" text="Dependencies"
                        description="Dependencies">
                    <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt shift D"/>
                </action>

            </group>
            <action id="ToMaven" class="com.dachen.creator.actions.ToMaven" text="ToMaven" description="ToMaven">
                <keyboard-shortcut keymap="$default" first-keystroke="ctrl alt shift M"/>
            </action>
            <separator/>
            <action id="CreateProject" class="com.dachen.creator.actions.NewProject" text="CreateProject"
                    description="CreateProject"/>
            <separator/>
            <action id="DpsAnalyze" class="com.dachen.creator.actions.DpsAnalyze" text="DpsAnalyze"
                    description="DpsAnalyze"/>
            <separator/>
            <action id="ClearCache" class="com.dachen.creator.actions.ClearCache" text="ClearCache" description="ClearCache"/>
            <separator/>
            <action id="com.libiao.routercreator" class="com.dachen.creator.actions.PathCreator" text="RouterCreator"
                    description="router auto create code"/>

            <action id="com.libiao.mvpcreator" class="com.dachen.creator.actions.MvpCreator" text="MvpCreator"
                    description="auto create MVP code"/>

            <action id="com.libiao.modelcreator" class="com.dachen.creator.actions.ModelCreator" text="ModelCreator"
                    description="auto create model code"/>
        </group>
        <!-- Add your actions here -->



    </actions>
</idea-plugin>